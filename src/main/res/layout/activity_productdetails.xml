<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".productdetails_Activity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="415dp"
        android:layout_height="152dp"
        app:srcCompat="@drawable/top_toolbar"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-4dp" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="291dp"
        android:layout_height="104dp"
        android:gravity="center_horizontal"
        android:text="Product Detail Form"
        android:textColor="@color/black"
        android:textSize="36sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="28dp" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="115dp"
        android:layout_height="34dp"
        android:text="Brand Name"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="198dp" />

    <EditText
        android:id="@+id/editTextText15"
        android:layout_width="252dp"
        android:layout_height="44dp"
        android:layout_marginHorizontal="16dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="enter brand name"
        android:inputType="text"
        android:padding="12dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="257dp" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wood Type"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="322dp" />

    <EditText
        android:id="@+id/editTextText16"
        android:layout_width="252dp"
        android:layout_height="44dp"
        android:layout_marginHorizontal="16dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="enter wood type"
        android:inputType="text"
        android:padding="12dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="373dp" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Discription"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="459dp" />

    <EditText
        android:id="@+id/editTextText17"
        android:layout_width="252dp"
        android:layout_height="44dp"
        android:layout_marginHorizontal="16dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="enter discription"
        android:inputType="text"
        android:padding="12dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="511dp" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wood Color"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="592dp" />

    <EditText
        android:id="@+id/editTextText18"
        android:layout_width="252dp"
        android:layout_height="44dp"
        android:layout_marginHorizontal="16dp"
        android:background="@drawable/edit_text_background"
        android:ems="10"
        android:hint="enter wood color"
        android:inputType="text"
        android:padding="12dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="648dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="138dp"
        android:layout_height="61dp"
        android:background="@drawable/new_button"
        android:text="SUBMIT"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="747dp" />

    <FrameLayout
        android:layout_width="413dp"
        android:layout_height="58dp"
        android:background="@color/green"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="860dp">
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>